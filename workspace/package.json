{
  "name": "workspace",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-b": "ng serve mf-b -o",
    "start-a": "ng serve mf-a -o",
    "start-s": "ng serve shell -o",

    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-lib": "ng build lk-common-lib --configuration production",
    "build-shell": "ng build --project shell --configuration production",
    "build-mf-a": "ng build --project mf-a --configuration production",
    "build-mf-b": "ng build --project mf-b --configuration production",
    "build-all": "npm run build-lib && npm run build-mf-a && npm run build-mf-b && npm run build-shell",
    "deploy": "npx angular-cli-ghpages --dir=dist",
    "deploy-gh": "gh-pages -d dist",
    "postbuild": "echo > dist/mf-with-native-federation/.nojekyll"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.4",
    "@angular/common": "^18.2.4",
    "@angular/compiler": "^18.2.4",
    "@angular/core": "^18.2.4",
    "@angular/forms": "^18.2.4",
    "@angular/platform-browser": "^18.2.4",
    "@angular/platform-browser-dynamic": "^18.2.4",
    "@angular/router": "^18.2.4",
    "@ng-bootstrap/ng-bootstrap": "^17.0.1",
    "es-module-shims": "^1.5.12",
    "rxjs": "~7.8.0",
    "tslib": "^2.7.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-architects/native-federation": "^18.2.2",
    "@angular-devkit/build-angular": "^18.2.4",
    "@angular/cli": "^18.2.4",
    "@angular/compiler-cli": "^18.2.4",
    "@types/jasmine": "~5.1.0",
    "angular-cli-ghpages": "^2.0.1",
    "gh-pages": "^6.1.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.2.1",
    "typescript": "~5.4.2"
  }
}
