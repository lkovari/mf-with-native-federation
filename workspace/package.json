{
  "name": "workspace",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-b": "ng serve mf-b -o",
    "start-a": "ng serve mf-a -o",
    "start-s": "ng serve shell -o",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-lib": "ng build lk-common-lib --configuration production",
    "build-shell": "ng build --project shell --configuration production",
    "build-mf-a": "ng build --project mf-a --configuration production",
    "build-mf-b": "ng build --project mf-b --configuration production",
    "copy-index": "cp ./index.html ./dist/index.html",
    "build-all": "npm run build-lib && npm run build-mf-a && npm run build-mf-b && npm run build-shell",
    "prepare-gh-pages": "rm -rf dist-gh && mkdir dist-gh && cp -r dist/shell/browser/* dist-gh/ && find dist/shell -mindepth 1 -maxdepth 1 ! -name browser -exec cp -r {} dist-gh/ \\; && mkdir -p dist-gh/mf-a && cp -r dist/mf-a/browser/* dist-gh/mf-a/ && find dist/mf-a -mindepth 1 -maxdepth 1 ! -name browser -exec cp -r {} dist-gh/mf-a/ \\; && mkdir -p dist-gh/mf-b && cp -r dist/mf-b/browser/* dist-gh/mf-b/ && find dist/mf-b -mindepth 1 -maxdepth 1 ! -name browser -exec cp -r {} dist-gh/mf-b/ \\; && touch dist-gh/.nojekyll",
    "deploy-gh": "npm run prepare-gh-pages && gh-pages -d dist-gh -b gh-pages",
    "postbuild": ""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.9",
    "@angular/common": "^19.2.9",
    "@angular/compiler": "^19.2.9",
    "@angular/core": "^19.2.9",
    "@angular/forms": "^19.2.9",
    "@angular/platform-browser": "^19.2.9",
    "@angular/platform-browser-dynamic": "^19.2.9",
    "@angular/router": "^19.2.9",
    "@ng-bootstrap/ng-bootstrap": "^18.0.0",
    "es-module-shims": "^2.4.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.7.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-architects/native-federation": "^19.0.22",
    "@angular-devkit/build-angular": "^19.2.10",
    "@angular/cli": "^19.2.10",
    "@angular/compiler-cli": "^19.2.9",
    "@types/jasmine": "~5.1.0",
    "angular-cli-ghpages": "^2.0.3",
    "gh-pages": "^6.3.0",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.2.2",
    "typescript": "~5.8.3"
  }
}
